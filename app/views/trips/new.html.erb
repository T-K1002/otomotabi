<%= form_with model: [@group, @trip], local: true do |f| %> 
    <div class="FormCountainer">
      <h2 class="NewTitle">旅行プラン作成<h2>
      <div class="FormTitle"> <%= f.label :旅行タイトル %> </br> <%= f.text_field :title %> </div>
      <div> <%= f.label :出発日 %> </br> <%= f.date_field :start_date %> </div>
      <div> <%= f.label :帰宅日 %> </br> <%= f.date_field :end_date %> </div>
      <div> <%= f.submit "保存" %> </div>
        <%= link_to "キャンセル", request.referer, class: "cancel_button" %>
      </div>
    </div>
  <% end %>
